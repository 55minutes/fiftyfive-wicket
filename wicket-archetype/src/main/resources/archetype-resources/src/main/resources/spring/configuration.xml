#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xmlns:tx="http://www.springframework.org/schema/tx"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd        http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">

  <!--
  - Load enviroment-specific properties. The properties file must be specified
  - in a system property called "${artifactId}.settings".
  - Use a -D${artifactId}.settings= command line argument
  - when running the application. This happens automatically with jetty:run.
  -->
  <bean class="fiftyfive.wicket.spring.PropertyPlaceholderAndLog4jConfigurer">
    <property name="key" value="${artifactId}.settings"/>
  </bean>

</beans>
