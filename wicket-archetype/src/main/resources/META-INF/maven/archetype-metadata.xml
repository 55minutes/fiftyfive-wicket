<?xml version="1.0" encoding="UTF-8"?><archetype-descriptor name="fiftyfive-wicket-sample">
  <requiredProperties>
    <requiredProperty key="project_name"/>
    <requiredProperty key="app_classname">
      <defaultValue>WicketApplication</defaultValue>
    </requiredProperty>
    <requiredProperty key="session_classname">
      <defaultValue>WicketSession</defaultValue>
    </requiredProperty>
  </requiredProperties>
  <fileSets>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="false" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.html</include>
        <include>**/*.txt</include>
        <include>**/*.css</include>
        <include>**/*.js</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/test/java</directory>
      <includes>
        <include>**/*.java</include>
        <include>**/*.html</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/webapp</directory>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>src/main/resources</directory>
    </fileSet>
  </fileSets>
</archetype-descriptor>